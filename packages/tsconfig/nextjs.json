{
  "extends": "./base.json",
  "display": "Next.js",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "plugins": [
    {
      "name": "next"
    }
  ],
  "paths": {
    "@/*": ["./*"]
  },
  "include": ["app", "next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"]
}
