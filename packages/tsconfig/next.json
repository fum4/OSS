{
  "extends": "./base.json",
  "display": "Next",
  "compilerOptions": {
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "app",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "paths": {
    "@/*": [
      "./*"
    ]
  },
  "plugins": [
    {
      "name": "next"
    }
  ]
}
