{
  "version": "2.1.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.5.1",
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "release": "pnpm lint && pnpm format && pnpm build && changeset && changeset version && changeset publish",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@nw/eslint-config": "workspace:*",
    "eslint": "latest",
    "jsdom": "^22.1.0",
    "prettier": "latest",
    "turbo": "latest",
    "vitest": "^0.31.1"
  }
}
