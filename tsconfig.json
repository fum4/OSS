{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "outDir": "lib",
    "jsx": "react",
    "allowJs": true,
    "declaration": true,
    "esModuleInterop": true
  },
  "include": [ "src/**/*" ],
  "exclude": [ "node_modules" ]
}
